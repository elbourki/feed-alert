{
  "auth": {
    "cognito": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false
    }
  },
  "analytics": {
    "pinpoint": {
      "service": "Pinpoint",
      "providerPlugin": "awscloudformation"
    }
  },
  "api": {
    "graphql": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 365,
              "description": "Guest users"
            }
          },
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "AMAZON_COGNITO_USER_POOLS",
              "userPoolConfig": {
                "userPoolId": "authcognito"
              }
            },
            {
              "authenticationType": "AWS_IAM"
            }
          ]
        }
      }
    },
    "rest": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "feeds",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "predictions": {
    "speech": {
      "providerPlugin": "awscloudformation",
      "service": "Polly",
      "convertType": "speechGenerator"
    }
  },
  "function": {
    "feeds": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "graphql",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ]
        }
      ]
    }
  }
}